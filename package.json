{
  "name": "project-reality",
  "displayName": "Project Reality: Syntax Highlighting",
  "description": "Syntax highlighting support for Project Reality: Battlefield 2",
  "publisher": "The Project Reality Team",
  "icon": "./icon.png",
  "version": "1.7.5",
  "engines": {
    "vscode": "^1.87.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "contributes": {
    "languages": [{
      "id": "pr",
      "aliases": ["Project Reality", "pr"],
      "extensions": [".tweak",".con",".ai",".inc", ".buildtemplate"],
      "configuration": "./language-configuration.json"
    }],
    "grammars": [{
      "language": "pr",
      "scopeName": "source.pr",
      "path": "./syntaxes/pr.tmLanguage.json"
    }]
  }
}
